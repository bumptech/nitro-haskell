g++ -m64 -O -fomit-frame-pointer -fPIC
g++ -m64 -fomit-frame-pointer -fPIC
g++ -m32 -O3 -fomit-frame-pointer -funroll-loops -fPIC
g++ -m32 -O -fomit-frame-pointer -fPIC
g++ -m32 -fomit-frame-pointer -fPIC
spu-g++ -mstdmain -march=cell -O3 -funroll-loops -fomit-frame-pointer -Drandom=rand -Dsrandom=srand -fPIC
